<Window x:Class="GraphicalCMake.DetailWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GraphicalCMake"
        mc:Ignorable="d"
        Title="DetailWindow" Height="180" Width="320">
    <Grid>
        <TextBlock TextWrapping="WrapWithOverflow" x:Name="DetailTb" d:LayoutOverrides="Width" Height="90" VerticalAlignment="Top"/>
        <CheckBox x:Name="isProject" Content="isProject" HorizontalAlignment="Right" Margin="0,0,10,38.901" VerticalAlignment="Bottom" Checked="isProject_Checked" Unchecked="isProject_Unchecked"/>

    </Grid>
</Window>
